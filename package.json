{
	"name": "obsidian-mcp-providers",
	"version": "1.0.0",
	"description": "MCP providers plugin for Obsidian.",
	"main": "main.js",
	"scripts": {
		"dev": "node --no-warnings=ExperimentalWarning esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node --no-warnings=ExperimentalWarning esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"format": "prettier --write .",
		"lint": "eslint . --ext .ts,.js",
		"test": "jest",
		"test:coverage": "jest --coverage"
	},
	"keywords": [],
	"author": "Pavel Frankov",
	"license": "MIT",
	"devDependencies": {
		"@testing-library/jest-dom": "^6.6.4",
		"@types/jest": "^30.0.0",
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "8.38.0",
		"@typescript-eslint/parser": "8.38.0",
		"builtin-modules": "5.0.0",
		"esbuild": "0.25.8",
		"jest": "^30.0.5",
		"jest-environment-jsdom": "^30.0.5",
		"obsidian": "latest",
		"prettier": "^3.6.2",
		"ts-jest": "^29.4.0",
		"tslib": "2.8.1",
		"typescript": "5.8.3"
	},
	"dependencies": {
		"@obsidian-ai-providers/sdk": "^1.3.0"
	}
}
